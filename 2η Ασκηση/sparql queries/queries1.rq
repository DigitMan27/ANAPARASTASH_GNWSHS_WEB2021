PREFIX univ: <http://www.mydomain.org/univ/>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/22-rdf-schema#>
SELECT ?name ?phone ?age
WHERE {
	?name univ:has_phone ?phone
	FILTER NOT EXISTS {?name univ:teaches ?value}
	?name univ:has_age ?age.
	FILTER(xsd:int(?age)>23)
}
